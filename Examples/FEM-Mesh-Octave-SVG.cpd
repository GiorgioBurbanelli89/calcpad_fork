"========================================================
"GRAFICACI√ìN DE MESH FEM CON OCTAVE - FORMATO SVG
"Rectangular Slab - Gr√°ficas vectoriales escalables
"========================================================

'<h2>Datos de Entrada</h2>

'Dimensiones de la losa
a = 6'm
b = 4'm

'Elementos del mesh
n_a = 6
n_b = 4

'Totales
n_e = n_a*n_b
n_j = (n_a + 1)*(n_b + 1)

'<hr>

'<h2>M√©todo 1: Mesh 2D en SVG (Vectorial Escalable)</h2>

@{octave}
% Datos del mesh
a = 6; b = 4; n_a = 6; n_b = 4;

% Generar coordenadas
[X, Y] = meshgrid(linspace(0, a, n_a+1), linspace(0, b, n_b+1));
x_nodes = X(:); y_nodes = Y(:);

% Conectividad
elements = [];
for i = 1:n_a
    for j = 1:n_b
        n1 = (i-1)*(n_b+1) + j;
        elements = [elements; n1 n1+(n_b+1) n1+(n_b+1)+1 n1+1];
    end
end

% Archivos de salida SVG
out_file = 'C:/Users/j-b-j/AppData/Local/Temp/fem_octave_svg1.svg';
gp_file = 'C:/Users/j-b-j/AppData/Local/Temp/fem_svg1.gp';

% Script gnuplot para SVG
fid = fopen(gp_file, 'w');
fprintf(fid, 'set terminal svg size 800,600 enhanced fname "Arial" fsize 11\n');
fprintf(fid, 'set output "%s"\n', out_file);
fprintf(fid, 'set title "Mesh FEM - SVG M√©todo 1: Visualizaci√≥n Vectorial 2D"\n');
fprintf(fid, 'set xlabel "X (m)"\nset ylabel "Y (m)"\nset grid\n');
fprintf(fid, 'set xrange [-0.5:%f]\nset yrange [-0.5:%f]\n', a+0.5, b+0.5);
fprintf(fid, 'set size ratio -1\n');
fprintf(fid, 'set style line 1 lc rgb "#008000" lw 2\n');

% Elementos
fprintf(fid, '$elem << EOD\n');
for e = 1:size(elements, 1)
    en = elements(e, :);
    for k = 1:4
        fprintf(fid, '%f %f\n', x_nodes(en(k)), y_nodes(en(k)));
        fprintf(fid, '%f %f\n', x_nodes(en(mod(k,4)+1)), y_nodes(en(mod(k,4)+1)));
        fprintf(fid, '\n');
    end
end
fprintf(fid, 'EOD\n');

% Apoyos
fprintf(fid, '$apoyos << EOD\n');
border = unique([find(x_nodes==0);find(x_nodes==a);find(y_nodes==0);find(y_nodes==b)]);
for i = 1:length(border)
    fprintf(fid, '%f %f\n', x_nodes(border(i)), y_nodes(border(i)));
end
fprintf(fid, 'EOD\n');

% Etiquetas
fprintf(fid, '$labels << EOD\n');
for e = 1:size(elements, 1)
    fprintf(fid, '%f %f %d\n', mean(x_nodes(elements(e,:))), mean(y_nodes(elements(e,:))), e);
end
fprintf(fid, 'EOD\n');

fprintf(fid, 'plot $elem w l ls 1 t "Elementos", $apoyos w p pt 7 ps 1.5 lc rgb "#ff0000" t "Apoyos", $labels u 1:2:3 w labels tc rgb "#006400" font "Arial,10" notitle\n');
fclose(fid);

[st, ~] = system(sprintf('gnuplot "%s"', gp_file));
if st == 0
    fprintf('<object data="%s" type="image/svg+xml" width="800" height="600" style="border:2px solid #0790c0; border-radius:8px; margin:10px 0;"></object>\n', out_file);
    fprintf('<p><strong>SVG Generado:</strong> Gr√°fica vectorial escalable sin p√©rdida de calidad</p>\n');
    fprintf('<p>Elementos: %d | Nodos: %d</p>\n', size(elements,1), length(x_nodes));
end
@{end octave}

'<hr>

