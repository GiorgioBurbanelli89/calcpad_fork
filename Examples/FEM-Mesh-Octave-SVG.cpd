fprintf(fid, 'plot $elem w l lw 2 lc rgb "#008000" t "Elementos", $nodos w p pt 7 ps 1.0 lc rgb "#ff4500" t "Nodos", $numeros u 1:2:3 w labels font "Arial,9" tc rgb "#8b0000" notitle\n');
