'<h1>Test Exhaustivo - Todos los Parsers CLI</h1>
'<p>Este test verifica TODOS los parsers disponibles en CLI</p>

'<h2>1. Python</h2>
@{python}
# Cálculo de área
radio = 5
area = 3.14159 * radio ** 2
print(f"Área del círculo: {area:.2f}")
@{end python}

'<h2>2. Octave</h2>
@{octave}
% Matriz simple
A = [1, 2; 3, 4];
det_A = det(A);
disp(['Determinante: ', num2str(det_A)]);
@{end octave}

'<h2>3. Fortran</h2>
@{fortran}
program test_fortran
    implicit none
    real :: x, y
    x = 10.0
    y = x * 2.0
    print *, 'Resultado:', y
end program test_fortran
@{end fortran}

'<h2>4. C</h2>
@{c}
#include <stdio.h>
int main() {
    int n = 42;
    printf("El número es: %d\n", n);
    return 0;
}
@{end c}

'<h2>5. C++ </h2>
@{cpp}
#include <iostream>
using namespace std;
int main() {
    double pi = 3.14159;
    cout << "Pi = " << pi << endl;
    return 0;
}
@{end cpp}

'<h2>6. C#</h2>
@{csharp}
using System;
class Program {
    static void Main() {
        double resultado = Math.Sqrt(16);
        Console.WriteLine($"Raíz cuadrada de 16: {resultado}");
    }
}
@{end csharp}

'<h2>7. Rust</h2>
@{rust}
fn main() {
    let x = 10;
    let y = 20;
    let suma = x + y;
    println!("Suma: {}", suma);
}
@{end rust}

'<h2>8. JavaScript</h2>
@{javascript}
// Función simple
function calcular() {
    const a = 5;
    const b = 3;
    return a * b;
}
console.log('Resultado:', calcular());
@{end javascript}

'<h2>9. TypeScript</h2>
@{typescript}
function sumar(a: number, b: number): number {
    return a + b;
}
const resultado = sumar(10, 20);
console.log(`Resultado: ${resultado}`);
@{end typescript}

'<h2>10. Go</h2>
